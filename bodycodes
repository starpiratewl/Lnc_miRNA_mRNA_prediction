cellines <- cellines[,-6]
colnames(cellines) <- c("Cancer","Posiont_Annotation","GeneSymbol","Correlation","Pvalue")

cellline <- cellines$GeneSymbol

intersect(cellline,MAPK)
intersect(cellline,PI3K)
